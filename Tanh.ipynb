   "source": [
    "class Tanh:\n",
    "    def tanh(self, input):\n",
    "        return (np.exp(input) - np.exp(-input)) / (np.exp(input) + np.exp(-input))\n",
    "    \n",
    "    def d_tanh(self, input):\n",
    "        return 1 - self.tanh(self.tanh(input)) ** 2"
